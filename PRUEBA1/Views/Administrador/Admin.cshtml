@{
}
<table>
    <tr>
        <th>Id</th>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Correo</th>
        <th>Contraseña</th>
        <th>Rol</th>
        <th>Acciones</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.IdUsuario</td>
            <td>@item.Nombre</td>
            <td>@item.Apellido</td>
            <td>@item.Correo</td>
            <td>@item.Contraseña</td>
            <td>@item.Rol.Nombre</td>
            <td>
                <a asp-controller="Administrador" asp-action="Editar">Editar</a>"                
                <a asp-controller="Administrador" asp-action="Eliminar">Editar</a>"
            </td>
        </tr>
    }
</table>
